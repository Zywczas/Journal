CREATE TABLE Video (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	timeStamp INTEGER NOT NULL,
	uri TEXT NOT NULL,
	description TEXT
);

getVideos:
SELECT *
FROM Video
ORDER BY timeStamp DESC
;

insertVideo:
INSERT OR REPLACE
INTO Video (
	id,
	timeStamp,
	uri,
	description
) VALUES ?;

insertVideoWithAutoGeneratedId:
INSERT
INTO Video (
	timeStamp,
	uri,
	description
) VALUES (?, ?, ?);
